{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "nodejs boilerplate for sequelize",
  "main": "server.js",
  "scripts": {
    "test": "npm run test",
    "start": "node_modules/.bin/babel-node --max-old-space-size:4096  -r dotenv/config server.js",
    "dev": "nodemon -r dotenv/config --exec node_modules/.bin/babel-node server.js",
    "prepare": "husky install",
    "build": "babel src --out-dir build"
  },
  "keywords": [
    "node",
    "sequelize",
    "orm",
    "boilerplate",
    "template",
    "mvc"
  ],
  "dependencies": {
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "firebase": "^9.13.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "prettier": "^2.8.0",
    "sequelize": "^6.20.1",
    "sequelize-cli": "^6.4.1",
    "sequelize-pg-utilities": "^2.0.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/preset-env": "^7.18.2",
    "husky": "^8.0.0",
    "nodemon": "^2.0.16",
    "pretty-quick": "^3.1.3"
  }
}
